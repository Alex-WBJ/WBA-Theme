{
  "components/common/footer": "{{#if banners.bottom}}\n    {{#if settings.data_tag_enabled}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            {{#each (limit banners.bottom_metadata 1)}}\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\n                    <div data-event-type=\"promotion-click\">\n                        {{{this.content}}}\n                    </div>\n                </div>\n            {{/each}}\n        </div>\n    {{else}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            <div class=\"banner\">\n                {{{limit banners.bottom 1}}}\n            </div>\n        </div>\n    {{/if}}\n\n{{/if}}\n<footer class=\"footer\" role=\"contentinfo\">\n    <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\n\n    <div class=\"footer-top\">\n        <div class=\"container\">\n            <div class=\"footer-row\">\n                <!-- Newsletter -->\n                <article class=\"footer-newsletter\" data-section-type=\"newsletterSubscription\">\n                    {{#if settings.show_newsletter_box}}\n                        {{> components/common/subscription-form}}\n                    {{/if}}\n                </article>\n        \n                <!-- Help & Support -->\n                <div class=\"footer-menu footer-menu--help-and-support\">\n                    <h3 class=\"footer-menu-title\">{{lang \"footer.help_and_support.title\"}}</h3>\n\n                    <ul class=\"footer-icon-menu-list\">\n                        <li>\n                            <a href=\"\">\n                                <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 18 16\"><path stroke=\"#fff\" stroke-linecap=\"round\" stroke-linejoin=\"round\" d=\"M12 11.1V1H1v10.1h11Zm0 0h5V7.2L14.8 5H12v6Zm-6 2c0 1-.7 1.9-1.7 1.9s-1.8-.9-1.8-2c0-1 .8-1.9 1.8-1.9s1.8.9 1.8 2Zm9.5 0c0 1-.8 1.9-1.8 1.9s-1.8-.9-1.8-2c0-1 .8-1.9 1.8-1.9s1.8.9 1.8 2Z\"/></svg>\n                                <span class=\"footer-icon-menu-item-label\">{{lang \"footer.help_and_support.delivery\"}}</span>\n                            </a>\n                        </li>\n\n                        <li>\n                            <a href=\"\">\n                                <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 22 22\"><path stroke=\"#fff\" stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M20.9 16v3a2 2 0 0 1-2.2 2 19.8 19.8 0 0 1-14.6-9.1 19.8 19.8 0 0 1-3-8.7A2 2 0 0 1 3 1h3a2 2 0 0 1 2 1.7c.1 1 .4 2 .7 2.8a2 2 0 0 1-.5 2.1L7 9a16 16 0 0 0 6 6l1.2-1.3a2 2 0 0 1 2.2-.4c.9.3 1.8.6 2.8.7a2 2 0 0 1 1.7 2Z\"/></svg>\n                                <span class=\"footer-icon-menu-item-label\">{{lang \"footer.help_and_support.call_us\"}}</span>\n                            </a>\n                        </li>\n\n                        <li>\n                            <a href=\"\">\n                                <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 22 18\"><path stroke=\"#fff\" stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M21 3a2 2 0 0 0-2-2H3a2 2 0 0 0-2 2m20 0v12a2 2 0 0 1-2 2H3a2 2 0 0 1-2-2V3m20 0-10 7L1 3\"/></svg>\n                                <span class=\"footer-icon-menu-item-label\">{{lang \"footer.help_and_support.email_us\"}}</span>\n                            </a>\n                        </li>\n\n                        <li>\n                            <a href=\"\">\n                                <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 27 22\"><path fill=\"#fff\" d=\"M23.9 8.9h-.6a9.8 9.8 0 0 0-19.6 0h-.6A3.1 3.1 0 0 0 0 12v3.8C0 17.6 1.4 19 3.1 19h1.5c.5 0 1-.5 1-1V9.8a8 8 0 0 1 15.9 0V18c0 .7-.6 1.3-1.3 1.3h-3c-.3-.5-.8-.9-1.5-.9-1 0-1.7.8-1.7 1.8s.8 1.8 1.7 1.8c.7 0 1.3-.3 1.6-.9h2.9c1.4 0 2.6-.9 3-2.1h.7c1.7 0 3.1-1.4 3.1-3.2V12c0-1.7-1.4-3.1-3.1-3.1ZM3.7 17H3c-.7 0-1.2-.6-1.2-1.3V12c0-.7.5-1.3 1.2-1.3h.6v6.4ZM25 15.8c0 .7-.5 1.3-1.2 1.3h-.6v-6.4h.6c.7 0 1.2.6 1.2 1.3v3.8Z\"/></svg>\n                                <span class=\"footer-icon-menu-item-label\">{{lang \"footer.help_and_support.support\"}}</span>\n                            </a>\n                        </li>\n\n                        <li>\n                            <a href=\"\">                                \n                                <svg width=\"20\" height=\"24\" viewBox=\"0 0 20 24\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                                    <path d=\"M19 10C19 17 10 23 10 23C10 23 1 17 1 10C1 7.61305 1.94821 5.32387 3.63604 3.63604C5.32387 1.94821 7.61305 1 10 1C12.3869 1 14.6761 1.94821 16.364 3.63604C18.0518 5.32387 19 7.61305 19 10Z\" stroke=\"white\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n                                    <path d=\"M10 13C11.6569 13 13 11.6569 13 10C13 8.34315 11.6569 7 10 7C8.34315 7 7 8.34315 7 10C7 11.6569 8.34315 13 10 13Z\" stroke=\"white\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n                                </svg>    \n                                <span class=\"footer-icon-menu-item-label\">{{lang \"footer.help_and_support.find_store\"}}</span>\n                            </a>\n                        </li>\n                    </ul>\n                </div>\n                <!--  -->\n        \n                <!-- Follow Us -->\n                <div class=\"footer-menu footer-menu--follow-us\">\n                    <h3 class=\"footer-menu-title\">{{lang \"footer.follow_us.title\"}}</h3>\n\n                    {{> components/common/social-links}}\n                </div>\n            </div>\n            {{#if theme_settings.show_seo_text}}\n            <div class=\"footer-row\">\n                <!-- SEO Text -->\n                <div class=\"footer-seo\">\n                    <h3 class=\"footer-seo-title\">{{lang \"footer.seo_text.title\"}}</h3>\n                    <div class=\"footer-seo-content\">\n                        {{{region name=\"footer-seo-text--global\"}}}\n                    </div>\n                    <a href=\"/about\" class=\"button footer-seo-toggle\">\n                        {{lang \"footer.seo_text.toggle\"}}\n                    </a>\n                </div>\n            </div>\n            {{/if}}\n        </div>\n    </div>\n\n    <div class=\"footer-bottom\">\n        <div class=\"container\">\n            <div class=\"footer-row --accordions\">\n                <!-- Find Us -->\n                <div class=\"footer-col\">\n                    <div class=\"footer-accordion --is-expanded --disabled\">\n                        <h3 class=\"footer-accordion-title\">{{lang \"footer.find_us.title\"}}</h3>\n\n                        <div class=\"footer-accordion-content\">\n                            <article class=\"footer-store\" data-section-type=\"storeInfo\">\n                                {{#if settings.address}}\n                                <div class=\"footer-store-info\">\n                                    <p>{{lang \"footer.find_us.address\"}}</p>\n                                    <address>{{nl2br settings.address}}</address>\n                                </div>\n                                {{/if}}\n                                \n                                <div class=\"footer-store-info\">\n                                    <p>{{lang \"footer.find_us.opening\"}}</p>\n                                    {{{region name=\"footer_opening_info--global\"}}}\n                                </div>\n                            </article>\n                        </div>\n                    </div>\n                </div>\n\n                <!-- Useful Resources -->\n                <div class=\"footer-col\">\n                    {{{region name=\"footer_useful_resources--global\"}}}\n                </div>\n\n                <!-- Customer Services -->\n                <div class=\"footer-col\">\n                    {{{region name=\"footer_customer_services--global\"}}}\n                </div>\n            </div>\n    \n            {{{region name=\"ssl_site_seal--global\"}}}\n\n            <!-- Copyright -->\n            {{#if theme_settings.show_copyright_footer}}\n                <div class=\"footer-copyright\">\n                    {{{region name=\"footer_copyright--global\"}}}\n                </div>\n            {{/if}}\n        </div>\n    </div>\n</footer>\n",
  "components/common/subscription-form": "<h3 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h3>\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix\">\n                <input class=\"form-input\"\n                       id=\"nl_email\"\n                       name=\"nl_email\"\n                       type=\"email\"\n                       value=\"{{customer.email}}\"\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n                       aria-describedby=\"alertBox-message-text\"\n                       aria-required=\"true\"\n                       required\n                >\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\"\n                       type=\"submit\"\n                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\n                >\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n\n<p class=\"newsletter-privacy\">{{{lang 'newsletter.secure'}}}</p>\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                    <svg>\n                        <use xlink:href=\"#icon-{{display_name}}\"/>\n                    </svg>\n\n                    <span>{{{capitalize display_name}}}</span>\n                </a>\n            </li>\n        {{/each}}\n\n        <li class=\"socialLinks-item\">\n            <a class=\"icon icon--tiktok\"\n               href=\"/\"\n               target=\"_blank\"\n               rel=\"noopener\"\n               title=\"TikTok\"\n            >\n                <svg role=\"img\" viewBox=\"0 0 24 24\" xmlns=\"http://www.w3.org/2000/svg\"><title>TikTok</title><path d=\"M12.525.02c1.31-.02 2.61-.01 3.91-.02.08 1.53.63 3.09 1.75 4.17 1.12 1.11 2.7 1.62 4.24 1.79v4.03c-1.44-.05-2.89-.35-4.2-.97-.57-.26-1.1-.59-1.62-.93-.01 2.92.01 5.84-.02 8.75-.08 1.4-.54 2.79-1.35 3.94-1.31 1.92-3.58 3.17-5.91 3.21-1.43.08-2.86-.31-4.08-1.03-2.02-1.19-3.44-3.37-3.65-5.71-.02-.5-.03-1-.01-1.49.18-1.9 1.12-3.72 2.58-4.96 1.66-1.44 3.98-2.13 6.15-1.72.02 1.48-.04 2.96-.04 4.44-.99-.32-2.15-.23-3.02.37-.63.41-1.11 1.04-1.36 1.75-.21.51-.15 1.07-.14 1.61.24 1.64 1.82 3.02 3.5 2.87 1.12-.01 2.19-.66 2.77-1.61.19-.33.4-.67.41-1.06.1-1.79.06-3.57.07-5.36.01-4.03-.01-8.05.02-12.07z\"/></svg>\n\n                <span>TikTok</span>\n            </a>\n        </li>\n    </ul>\n{{/if}}\n"
}
