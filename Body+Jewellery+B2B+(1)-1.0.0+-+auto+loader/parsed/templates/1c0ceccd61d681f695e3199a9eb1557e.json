{
  "components/faceted-search/facets/range": "<div class=\"accordion-block\">\n    {{> components/faceted-search/faceted-search-navigation}}\n\n    <div id=\"facetedSearch-content--{{dashcase facet}}\" class=\"accordion-content{{#unless start_collapsed}} is-open{{/unless}}\">\n        <div class=\"slide-out-title\" data-collapsible=\"#facetedSearch-content--{{dashcase facet}}\">\n            <svg width=\"11\" height=\"16\" viewBox=\"0 0 11 16\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                <path d=\"M8 13L3 8L8 3\" stroke=\"white\" stroke-width=\"3\" stroke-linecap=\"square\"/>\n            </svg>\n    \n            {{title}}\n        </div>\n        <form id=\"facet-range-form\" class=\"form\" method=\"get\" data-faceted-search-range novalidate>\n            <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\">\n            {{#if this.sort}}\n                <input type=\"hidden\" name=\"sort\" value=\"{{this.sort}}\">\n            {{/if}}\n            <fieldset class=\"form-fieldset\">\n                <div class=\"form-minMaxRow\">\n                    <div class=\"form-field\">\n                        <input\n                            name=\"min_price\"\n                            placeholder=\"{{lang 'search.faceted.range.min-placeholder'}}\"\n                            min=\"0\"\n                            class=\"form-input form-input--small\"\n                            required\n                            type=\"number\"\n                            value=\"{{min_price}}\"\n                        />\n                    </div>\n\n                    <div class=\"form-field\">\n                        <input\n                            name=\"max_price\"\n                            placeholder=\"{{lang 'search.faceted.range.max-placeholder'}}\"\n                            min=\"0\"\n                            class=\"form-input form-input--small\"\n                            required\n                            type=\"number\"\n                            value=\"{{max_price}}\"\n                        />\n                    </div>\n\n                    <div class=\"form-field\">\n                        <button class=\"button button--small\" type=\"submit\">\n                            {{lang 'search.faceted.range.update'}}\n                        </button>\n                    </div>\n                </div>\n\n                <div class=\"form-inlineMessage\"></div>\n            </fieldset>\n        </form>\n        <div class=\"buttons-wrapper\" data-close-filters style=\"display: none;\">\n            <button class=\"button button-view-products\">\n                View Products\n            </button>\n        </div>\n    </div>\n\n</div>\n",
  "components/faceted-search/faceted-search-navigation": "<div class=\"accordion-nav-clear-holder\">\n    <button type=\"button\"\n         class=\"accordion-navigation toggleLink{{#unless start_collapsed}} is-open{{/unless}}\"\n         data-collapsible=\"#facetedSearch-content--{{dashcase facet}}\"\n         aria-label=\"{{lang 'search.faceted.toggleSection' title=title}}\"\n    >\n        <span class=\"accordion-title\">\n            {{title}} \n        </span>\n\n        <span>\n            <svg width=\"9\" height=\"16\" viewBox=\"0 0 9 16\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n            <path d=\"M2 14L8 8L2 2\" stroke=\"#171718\" stroke-width=\"2\" stroke-linecap=\"square\" stroke-linejoin=\"round\"/>\n            </svg>\n            \n        </span>\n    </button>\n    {{#if selected}}\n        <a href=\"{{remove_url}}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\n    {{/if}}\n</div>\n"
}
