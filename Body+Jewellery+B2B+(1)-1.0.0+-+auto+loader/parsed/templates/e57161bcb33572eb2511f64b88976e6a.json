{
  "components/common/paginator": "<nav class=\"pagination\" aria-label=\"pagination\"></nav>\n    {{#if total}}\n    <p class=\"pagination-summary\">\n        {{{lang \"category.viewed\" upto=end total=total}}}\n    </p>\n    {{/if}}\n\n    <ul class=\"pagination-list\">\n        {{#if previous}}\n            <li class=\"pagination-item pagination-item--previous\">\n                <a class=\"pagination-link\"\n                   href=\"{{previous}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.previous'}}\"\n                >                    \n                    <svg width=\"13\" height=\"19\" viewBox=\"0 0 13 19\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                        <path d=\"M9.45605 15.1973L3.10671 9.37703L9.45606 3.5568\" stroke=\"white\" stroke-width=\"4\" stroke-linecap=\"square\"/>\n                    </svg>    \n                </a>\n            </li>\n        {{/if}}\n        {{#each links}}\n            <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\n                <a class=\"pagination-link\"\n                   href=\"{{this.url}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   {{#if this.number '==' ../current}}\n                   aria-current=\"page\"\n                   data-pagination-current-page-link\n                   {{/if}}\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\n                >\n                    {{this.number}}\n                </a>\n            </li>\n        {{/each}}\n\n        {{#if next}}\n            <li class=\"pagination-item pagination-item--next\">\n                <a class=\"pagination-link\"\n                   href=\"{{next}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.next'}}\"\n                >\n                    <svg width=\"13\" height=\"19\" viewBox=\"0 0 13 19\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                        <path d=\"M3.42822 3.55676L9.77757 9.377L3.42822 15.1972\" stroke=\"white\" stroke-width=\"4\" stroke-linecap=\"square\"/>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n"
}
