  "components/account/navigation": "<nav class=\"navBar navBar--sub navBar--account navBar--mobileSelect\">\n    <ul class=\"navBar-section\">\n        <li class=\"navBar-item {{#if account_page '===' 'orders'}}is-active{{/if}}\">\n            <a class=\"navBar-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n        </li>\n        {{#if settings.returns_enabled}}\n            <li class=\"navBar-item {{#if account_page '===' 'returns'}}is-active{{/if}}\">\n                <a class=\"navBar-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n            </li>\n        {{/if}}\n        <li class=\"navBar-item {{#if account_page '===' 'messages'}}is-active{{/if}}\">\n            <a class=\"navBar-action\" href=\"{{urls.account.inbox}}\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</a>\n        </li>\n        <li class=\"navBar-item {{#if account_page '===' 'addresses'}}is-active{{/if}}\">\n            <a class=\"navBar-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\n        </li>\n        {{#if settings.show_payment_methods}}\n            <li class=\"navBar-item {{#if account_page '===' 'payment_methods'}}is-active{{/if}}\">\n                <a class=\"navBar-action\" href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\n            </li>\n        {{/if}}\n        {{#if settings.show_wishlist}}\n            <li class=\"navBar-item {{#if account_page '===' 'wishlists'}}is-active{{/if}}\">\n                <a class=\"navBar-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.nav.wishlists'}}</a>\n            </li>\n        {{/if}}\n        <li class=\"navBar-item {{#if account_page '===' 'recent_items'}}is-active{{/if}}\">\n            <a class=\"navBar-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n        </li>\n        <li class=\"navBar-item {{#if account_page '===' 'settings'}}is-active{{/if}}\">\n            <a class=\"navBar-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n        </li>\n    </ul>\n\n    <select data-nav-select>\n        <option value=\"{{urls.account.orders.all}}\" {{#if account_page '===' 'orders'}}selected{{/if}}>{{lang 'account.nav.orders'}}</option>\n        {{#if settings.returns_enabled}}\n        <option value=\"{{urls.account.returns}}\" {{#if account_page '===' 'returns'}}selected{{/if}}>{{lang 'account.nav.returns'}}</option>\n        {{/if}}\n        <option value=\"{{urls.account.inbox}}\" {{#if account_page '===' 'messages'}}selected{{/if}}>{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</option>\n        <option value=\"{{urls.account.addresses}}\" {{#if account_page '===' 'addresses'}}selected{{/if}}>{{lang 'account.nav.addresses'}}</option>\n        {{#if settings.show_payment_methods}}\n        <option value=\"{{urls.account.payment_methods.all}}\" {{#if account_page '===' 'payment_methods'}}selected{{/if}}>{{lang 'account.nav.payment_methods'}}</option>\n        {{/if}}\n        {{#if settings.show_wishlist}}\n        <option value=\"{{urls.account.wishlists.all}}\" {{#if account_page '===' 'wishlists'}}selected{{/if}}>{{lang 'account.nav.wishlists'}}</option>\n        {{/if}}\n        <option value=\"{{urls.account.recent_items}}\" {{#if account_page '===' 'recent_items'}}selected{{/if}}>{{lang 'account.nav.recently_viewed'}}</option>\n        <option value=\"{{urls.account.details}}\" {{#if account_page '===' 'settings'}}selected{{/if}}>{{lang 'account.nav.settings'}}</option>\n    </select>\n</nav>\n"
