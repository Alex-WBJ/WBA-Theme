\"\n                                                    >\n                                                        {{name}}\n                                                    </a>\n                                                </li>\n                                            {{/each}}\n                                        </ul>\n                                    </div>\n                                {{/if}}\n                            </li>\n                            {{/each}}\n                        </ul>\n                    </div>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n\n    <div data-expanding-menu=\"{{id}}\"></div>\n\n    <div class=\"navPage-popular\">\n        <div class=\"wrap hidden\">\n            <h2>Most Popular</h2>\n\n            <div class=\"navPage-popular-listing\" data-popular-products></div>\n            \n        </div>\n    </div>\n</div>\n{{else}}\n  {{#or (if name '===' \"Top Sellers\")}}\n    {{else}}\n        <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n        href=\"{{url}}\"\n        aria-label=\"{{name}}\"\n        >\n            <span class=\"navPages-action-text\">{{name}}</span>\n        </a>\n    {{/or}}\n{{/if}}\n",
