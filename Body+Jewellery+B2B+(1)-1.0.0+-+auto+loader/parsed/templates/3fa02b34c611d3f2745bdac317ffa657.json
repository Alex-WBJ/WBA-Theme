{
  "components/products/add-to-cart": "<div id=\"add-to-cart-wrapper\" class=\"add-to-cart-wrapper\" {{#unless product.can_purchase}}style=\"display: none\"{{/unless}}>\n    {{#if theme_settings.show_product_quantity_box}}\n        {{inject 'productQuantityErrorMessage'  (lang 'products.quantity_error_message')}}\n        <div class=\"form-field form-field--increments\">\n            <label class=\"form-label form-label--alternate\"\n                   for=\"qty[]\">{{lang 'products.quantity'}}</label>\n                <div class=\"form-increment\" data-quantity-change>\n                    <div class=\"field-group\">\n                        <button class=\"button button--icon\" data-action=\"dec\">\n                            <span class=\"is-srOnly\">{{lang 'products.quantity_decrease' name=product.title}}</span>\n                            <i class=\"icon\" aria-hidden=\"true\">\n                                <svg>\n                                    <use xlink:href=\"#icon-keyboard-arrow-down\"/>\n                                </svg>\n                            </i>\n                        </button>\n                        <input class=\"form-input form-input--incrementTotal\"\n                            id=\"qty[]\"\n                            name=\"qty[]\"\n                            type=\"tel\"\n                            value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\n                            data-quantity-min=\"{{product.min_purchase_quantity}}\"\n                            data-quantity-max=\"{{product.max_purchase_quantity}}\"\n                            min=\"1\"\n                            pattern=\"[0-9]*\"\n                            aria-live=\"polite\">\n                        <button class=\"button button--icon\" data-action=\"inc\">\n                            <span class=\"is-srOnly\">{{lang 'products.quantity_increase' name=product.title}}</span>\n                            <i class=\"icon\" aria-hidden=\"true\">\n                                <svg>\n                                    <use xlink:href=\"#icon-keyboard-arrow-up\"/>\n                                </svg>\n                            </i>\n                        </button>\n                    </div>\n                </div>\n        </div>\n    {{/if}}\n\n    <div class=\"alertBox productAttributes-message\" style=\"display:none\">\n        <div class=\"alertBox-column alertBox-icon\">\n            <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n        </div>\n        <p class=\"alertBox-column alertBox-message\"></p>\n    </div>\n    {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n        <div class=\"form-action\">\n            <button\n                    id=\"form-action-addToCart\"\n                    data-wait-message=\"{{lang 'products.adding_to_cart'}}\"\n                    class=\"button button--primary\"\n                    type=\"submit\"\n                    value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\"\n            >\n                <svg width=\"22\" height=\"21\" viewBox=\"0 0 22 21\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                    <path d=\"M1 1H4.63636L7.07273 13.1148C7.15586 13.5313 7.38355 13.9055 7.71595 14.1718C8.04835 14.4381 8.46427 14.5796 8.89091 14.5714H17.7273C18.1539 14.5796 18.5698 14.4381 18.9022 14.1718C19.2346 13.9055 19.4623 13.5313 19.5455 13.1148L21 5.52381H5.54545M9.18182 19.0952C9.18182 19.5949 8.7748 20 8.27273 20C7.77065 20 7.36364 19.5949 7.36364 19.0952C7.36364 18.5956 7.77065 18.1905 8.27273 18.1905C8.7748 18.1905 9.18182 18.5956 9.18182 19.0952ZM19.1818 19.0952C19.1818 19.5949 18.7748 20 18.2727 20C17.7707 20 17.3636 19.5949 17.3636 19.0952C17.3636 18.5956 17.7707 18.1905 18.2727 18.1905C18.7748 18.1905 19.1818 18.5956 19.1818 19.0952Z\" stroke=\"white\" stroke-width=\"1.5\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n                </svg>\n                <span>{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}</span>\n            </button>\n            <span class=\"product-status-message aria-description--hidden\">{{lang 'products.adding_to_cart'}} {{lang 'category.add_cart_announcement'}}</span>\n        </div>\n    {{/or}}\n</div>\n"
}
