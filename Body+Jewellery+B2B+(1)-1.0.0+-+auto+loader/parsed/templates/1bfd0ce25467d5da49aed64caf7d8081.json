{
  "components/common/navigation-list": "{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   data-navid=\"navPages-{{id}}\"\n>\n    <span class=\"navPages-action-text\">{{name}}</span>\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg class=\"icon-toggle\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <div class=\"navPages-title--mobile\"\n         data-navid=\"navPages-{{id}}\">\n        <i class=\"icon\" aria-hidden=\"true\">\n            <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n        </i>\n        <span>{{name}}</span>\n    </div>\n\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item --all\">\n            <a class=\"navPage-subMenu-action navPages-action\"\n               href=\"{{url}}\"\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n            >\n                {{lang 'category.view_all.name' category=name}}\n            </a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                       data-navid=\"navPages-{{id}}\"\n                    >\n                        <span class=\"navPages-action-text\">{{name}}</span>\n                        <span class=\"collapsible-icon-wrapper\"\n                            data-navid=\"navPages-{{id}}\"\n                            data-navid-disabled-breakpoint=\"medium\"\n                            data-navid-disabled-state=\"open\"\n                            data-navid-enabled-state=\"closed\"\n                        >\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                <svg class=\"icon-toggle\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                            </i>\n                        </span>\n                    </a>\n                    <div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                        <div class=\"navPages-title--mobile\"\n                             data-navid=\"navPages-{{id}}\">\n                            <i class=\"icon\" aria-hidden=\"true\">\n                                <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n                            </i>\n                            <span>{{name}}</span>\n                        </div>\n\n                        <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                            {{#each children}}\n                            <li class=\"navPage-childList-item\"\n                                {{#if children}}\n                                    data-child-id=\"{{id}}\" \n                                    data-child-data=\"{{json children}}\" \n                                    data-category-title=\"{{./name}}\"\n                                    data-category-url=\"{{./url}}\"\n                                {{/if}} \n                                data-depth=\"1\"\n                            >\n                                <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}} {{#if children}}has-subMenu{{/if}}\"\n                                href=\"{{url}}\"\n                                aria-label=\"{{name}}\"\n                                data-navid=\"navPages-{{./id}}\"\n                                >\n                                    <span class=\"navPages-action-text\">{{name}}</span>\n                                    <span class=\"collapsible-icon-wrapper\"\n                                        data-navid=\"navPages-{{./id}}\"\n                                        data-navid-disabled-breakpoint=\"medium\"\n                                        data-navid-disabled-state=\"open\"\n                                        data-navid-enabled-state=\"closed\"\n                                    >\n                                    {{#if children}}\n                                        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                            <svg class=\"icon-toggle\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                                        </i>\n                                    {{/if}}\n                                </a>\n                                {{#if children}}\n                                    <div class=\"navPage-subMenu\" id=\"navPages-{{./id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                                        <div class=\"navPages-title--mobile\"\n                                            data-navid=\"navPages-{{./id}}\">\n                                            <i class=\"icon\" aria-hidden=\"true\">\n                                                <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n                                            </i>\n                                            <span>{{name}}</span>\n                                        </div>\n\n                                        <ul class=\"navPage-childList navPage-childList-subMenu\" id=\"navPages-{{id}}\" data-depth=\"2\">\n                                            {{#each children}}\n                                                <li class=\"navPage-childList-item\">\n                                                    <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                                                    href=\"{{url}}\"\n                                                    aria-label=\"{{name}}\"\n                                                    >\n                                                        {{name}}\n                                                    </a>\n                                                </li>\n                                            {{/each}}\n                                        </ul>\n                                    </div>\n                                {{/if}}\n                            </li>\n                            {{/each}}\n                        </ul>\n                    </div>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n\n    <div data-expanding-menu=\"{{id}}\"></div>\n\n    <div class=\"navPage-popular\">\n        <div class=\"wrap hidden\">\n            <h2>Most Popular</h2>\n\n            <div class=\"navPage-popular-listing\" data-popular-products></div>\n            \n        </div>\n    </div>\n</div>\n{{else}}\n  {{#or (if name '===' \"Top Sellers\")}}\n    {{else}}\n        <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n        href=\"{{url}}\"\n        aria-label=\"{{name}}\"\n        >\n            <span class=\"navPages-action-text\">{{name}}</span>\n        </a>\n    {{/or}}\n{{/if}}\n"
}
