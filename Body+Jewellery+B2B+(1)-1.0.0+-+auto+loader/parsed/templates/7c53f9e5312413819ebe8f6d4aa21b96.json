  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                    <svg>\n                        <use xlink:href=\"#icon-{{display_name}}\"/>\n                    </svg>\n\n                    <span>{{{capitalize display_name}}}</span>\n                </a>\n            </li>\n        {{/each}}\n\n        <li class=\"socialLinks-item\">\n            <a class=\"icon icon--tiktok\"\n               href=\"/\"\n               target=\"_blank\"\n               rel=\"noopener\"\n               title=\"TikTok\"\n            >\n                <svg role=\"img\" viewBox=\"0 0 24 24\" xmlns=\"http://www.w3.org/2000/svg\"><title>TikTok</title><path d=\"M12.525.02c1.31-.02 2.61-.01 3.91-.02.08 1.53.63 3.09 1.75 4.17 1.12 1.11 2.7 1.62 4.24 1.79v4.03c-1.44-.05-2.89-.35-4.2-.97-.57-.26-1.1-.59-1.62-.93-.01 2.92.01 5.84-.02 8.75-.08 1.4-.54 2.79-1.35 3.94-1.31 1.92-3.58 3.17-5.91 3.21-1.43.08-2.86-.31-4.08-1.03-2.02-1.19-3.44-3.37-3.65-5.71-.02-.5-.03-1-.01-1.49.18-1.9 1.12-3.72 2.58-4.96 1.66-1.44 3.98-2.13 6.15-1.72.02 1.48-.04 2.96-.04 4.44-.99-.32-2.15-.23-3.02.37-.63.41-1.11 1.04-1.36 1.75-.21.51-.15 1.07-.14 1.61.24 1.64 1.82 3.02 3.5 2.87 1.12-.01 2.19-.66 2.77-1.61.19-.33.4-.67.41-1.06.1-1.79.06-3.57.07-5.36.01-4.03-.01-8.05.02-12.07z\"/></svg>\n\n                <span>TikTok</span>\n            </a>\n        </li>\n    </ul>\n{{/if}}\n",
  "components/common/currency-selector": "<!-- #if currency_selector.currencies.length '>' 1 -->\n{{#if true}}\n<li class=\"navUser-item\">\n    <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n        href=\"#\"\n        data-dropdown=\"currencySelection\"\n        aria-controls=\"currencySelection\"\n        aria-expanded=\"false\"\n    >\n\n        <span class=\"navUser-item-text\">\n            <span class=\"navUser-item-topline\">{{lang \"header.nav.currency.currency\" }}</span>\n            <span class=\"navUser-item-mainline\">{{settings.money.currency_token}} {{ currency_selector.active_currency_code }}</span>\n        </span>\n\n        <svg class=\"navUser-action-dropdown-icon\" width=\"14\" height=\"8\" viewBox=\"0 0 14 8\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n            <path d=\"M1 1L7 7L13 1\" stroke=\"white\" stroke-width=\"1.5\" stroke-linecap=\"round\" stroke-linejoin=\"round\" fill=\"none\"/>\n        </svg>\n    </a>\n    <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n        {{#each currency_selector.currencies}}\n        <li class=\"dropdown-menu-item\">\n            <a href=\"{{{switch_url}}}\"\n                data-currency-code=\"{{{code}}}\"\n                {{#if cart_currency_switch_url.length '>' 0 }}\n                data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                {{/if}}\n                data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n            >\n            {{#if is_active}}\n                <strong>{{name}}</strong>\n            {{else}}\n                {{name}}\n            {{/if}}\n            </a>\n        </li>\n        {{/each}}\n    </ul>\n</li>\n{{/if}}\n"
